{
  "name": "pm2web",
  "version": "0.0.1",
  "main": "index.js",
  "repository": {},
  "author": "eseom <me@eseom.org>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node bin/server",
    "dev": "NODE_ENV=development rm webpack-assets.json; concurrently --kill-others \"yarn dev:packager\" \"yarn dev:server\"",
    "dev:packager": "node bin/packager",
    "dev:server": "node bin/server",
    "build": "node -r ./require-ts node_modules/.bin/webpack --verbose --colors --display-error-details --config webpack/production.ts"
  },
  "dependencies": {
    "concurrently": "^3.3.0",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@types/hapi": "^16.0.0",
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.0.4",
    "clean-webpack-plugin": "^0.1.15",
    "piping": "^1.0.0-rc.4",
    "react-hot-loader": "3.0.0-beta.6",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-isomorphic-tools": "^2.6.6"
  }
}
